    "@typescript-eslint/parser": "6.7.2",
    "@types/node": "20.8.9",
    "autoprefixer": "10.4.16",
    "cssnano": "5.1.15",
    "eslint": "8.50.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
